ass=\"{one: bar.value == 1, toolow: bar.value < findMax(bars) / 10}\" :style=\"{'--num': bars.length,'--height': + bar.value}\"><p>{{bar.tag}}</p><p class=\"hover-graph\">{{findDate((bars.length - 1) - index)}}</p></div>\r\n        <p :class=\"{small: small}\" class=\"yAxis\">{{y_axis}}</p>\r\n      </div>\r\n      <div v-if=\"override && !none\" class=\"graph-labels\">\r\n        <p>{{findDate(bars.length - 1)}}</p>\r\n        <p>{{findDate(0)}}</p>\r\n      </div>\r\n      <h4 id=\"nosongs\" v-if=\"none\">No Songs Liked</h4>\r\n    </div>\r\n    <div class=\"choosebutton\" v-if=\"save\" @click=\"toggleSave\" :class=\"{add: !state, remove: state}\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Loading from '@/components/General/Loading.vue'\r\n\r\nexport default {\r\n  name: 'timeline',\r\n  components: {\r\n    Loading\r\n  },\r\n  props: {\r\n    title: String,\r\n    delay: Number,\r\n    bars: Array,\r\n    y_axis: String,\r\n    color: Object,\r\n    override: Boolean,\r\n    max: Number,\r\n    small: Boolean,\r\n    none: Boolean,\r\n    save: Boolean,\r\n    state: Boolean,\r\n  },\r\n  methods: {\r\n    toggleSave() {\r\n      this.$emit('toggleSave');\r\n    },\r\n    findDate(month) {\r\n        let now = new Date();\r\n        let nowMonth = now.getMonth();\r\n        let year = 0;\r\n        nowMonth -= month;\r\n        nowMonth -= 1;\r\n        while (nowMonth < 0) {\r\n            year += 1;\r\n            nowMonth = 12 + nowMonth;\r\n        } \r\n        nowMonth += 1;\r\n        let returnYear = (now.getFullYear() - year) % 100;\r\n        return nowMonth + \"/\" + returnYear;\r\n    },\r\n    findMax(array) {\r\n      if (this.max != -1) {\r\n        return this.max;\r\n      }\r\n      let max = 0;\r\n      for (var i = 0; i < array.length; i++)\r\n      {\r\n        if (array[i].value > max)\r\n          max = array[i].value;\r\n      }\r\n      return max;\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute 
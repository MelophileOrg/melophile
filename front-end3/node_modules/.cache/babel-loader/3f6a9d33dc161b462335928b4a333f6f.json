VA;AARA,CAAA","sourcesContent":["<template>\r\n    <div class=\"percentilebar\">\r\n        <h4 class=\"bar-title\">{{title}}</h4>\r\n        <div class=\"stat-bar\">\r\n            <div class=\"fill\" :style=\"{'--percent': + percent, '--red': + color.red, '--green': + color.green, '--blue': + color.blue}\"/>\r\n            <div class=\"fill excess\" :style=\"{'--percent': + percent, '--red': + color.red - 60, '--green': + color.green - 60, '--blue': + color.blue - 60}\"/>\r\n        </div>\r\n        <h5 class=\"value\">{{value(percent)}}</h5>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'percentilebar',\r\n  props: {\r\n    title: String,\r\n    percent: Number,\r\n    color: Object,\r\n    type: String,\r\n  },\r\n  methods: {\r\n    percentprint(num) {\r\n        return Math.round(num * 100) + \"%\";\r\n    },\r\n    value() {\r\n      if (this.type == \"tempo\")\r\n        return Math.round(this.percent * 208) + \"BPM\";\r\n      if (this.type == \"volume\")\r\n        return (Math.round(((this.percent * 50) - 50) * 10) / 10) + \"dB\";\r\n      return Math.round(this.percent * 100) + \"%\"\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.percentilebar {\r\n  --delay: 3;\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 10px;\r\n  justify-content: space-between;\r\n}\r\n\r\n.bar-title {\r\n  width: 115px;\r\n}\r\n\r\n.stat-bar {\r\n    display: flex;\r\n    border-radius: 5px;\r\n    overflow: hidden;\r\n    max-width: 200px;\r\n    width: calc(100% - 155px);\r\n    max-height: 10px;\r\n    background-color: rgba(255, 255, 255, 0.247);\r\n}\r\n\r\n.stat-bar .fill {\r\n    --percent: 0;\r\n    --red: 255;\r\n    --green: 255;\r\n    --blue: 255;\r\n    display: block;\r\n    width: calc(var(--percent) * 100%);\r\n    height: 10px;\r\n    background: rgb(var(--red), var(--green), var(--blue));\r\n    animation: slow-fill 1s ease calc(var(--delay) * .1s);\r\n}\r\n\r\n.stat-bar .fill.excess {\r\n  width: calc((1 - var(--percent)) * 100%);\r\n  border-left: 2px solid white;\r\n  animation: slow-fill-reverse 1s ease calc(var(--delay) * .1s), border-throb 1s ease-in-out calc(var(--delay) * .1s) infinite;\r\n  \r\n}\r\n\r\n@keyframes border-throb {\r\n  50% {\r\n    border-left: 2px solid rgb(var(--red), var(--green), var(--blue));\r\n    \r\n  }\r\n}\r\n\r\n@keyframes slow-fill {\r\n  from {\r\n    width: calc(var(--percent) * 0%);\r\n  }\r\n}\r\n\r\n@keyframes slow-fill-reverse {\r\n  from {\r\n    width: calc(100%);\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 720px) {\r\n    h4 {\r\n        font-size: .9em !important;\r\n    }\r\n\r\n    .bar-title {\r\n      width: 110px !important;\r\n    }\r\n\r\n    .stat-bar {\r\n        width: calc(100% - 150px);\r\n    }\r\n\r\n    h5 {\r\n      font-size: .9em !important;\r\n    }\r\n}\r\n\r\nh4 {\r\n  color: white;\r\n  display: flex;\r\n  align-items: center;\r\n  margin: 4px 0;\r\n  text-align: left;\r\n}\r\n\r\nh5 {\r\n  color: white;\r\n  margin: 4px 0;\r\n  text-align: right;\r\n}\r\n\r\n.bar-title {\r\n  width: 115px;\r\n}\r\n\r\n.value {\r\n  width: 63px;\r\n  font-size: 16px;\r\n}\r\n</style>"],"sourceRoot":"src/components/Analysis"}]}
de-up .5s ease calc(var(--delay) * .1s), peekaboo calc(var(--delay) * .1s) linear, throb-row 2s ease-in-out calc(var(--delay) * .23s + var(--delay) * .1s + .5s) infinite;\r\n}\r\n\r\n@keyframes throb-row {\r\n  0%{\r\n    opacity: 1;\r\n  }\r\n  50%{\r\n    opacity: .3;\r\n  }\r\n  100%{\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n@media screen and (min-width: 500px) {\r\n  .load#options {\r\n    margin-top: 0vh !important;\r\n  }\r\n}\r\n\r\n.load#options {\r\n  margin-top: 40px;\r\n}\r\n\r\n#options {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  align-items: center;\r\n  margin: 32px auto;\r\n  margin-top: 40px;\r\n  animation: slide-up .3s ease .3s, peekaboo .3s linear;\r\n}\r\n\r\n#options h1 {\r\n  color: rgba(252, 252, 252, 0.301);\r\n  text-shadow: 1px 1px 10px rgba(255, 255, 255, 0.075);\r\n  font-size: 40px;\r\n  font-weight: lighter;\r\n  margin: 0px 15px;\r\n  font-family: 'Bitter', serif;\r\n  font-family: 'Roboto', sans-serif;\r\n}\r\n\r\n.row h1 {\r\n  color: white;\r\n  margin: 0 10px;\r\n  font-weight: lighter;\r\n  font-size: 28px;\r\n  text-align: left;\r\n}\r\n\r\n\r\n.row h2 {\r\n  color: rgb(250, 250, 250);\r\n  margin: 0 10px 0px 0px;\r\n  font-weight: bolder;\r\n  font-size: 20px;\r\n  width: 20px;\r\n}\r\n\r\n\r\n.image {\r\n  display: block;\r\n  width: 60px;\r\n  height: 60px;\r\n  background-size: auto 100%;\r\n  background-position: center center;\r\n}\r\n\r\n\r\n\r\n\r\n</style>"],"sourceRoot":"src/views"}]}
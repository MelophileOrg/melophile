ow: false,\n      index: -1,\n      hover: false\n    };\n  },\n  methods: {\n    hovering: function hovering() {\n      this.hover = true;\n    },\n    leave: function leave() {\n      this.hover = false;\n    },\n    open: function open() {\n      this.show = true;\n      this.$emit('pending');\n    },\n    close: function close() {\n      this.show = false;\n    },\n    toggle: function toggle() {\n      if (this.show) {\n        this.index = -1;\n        this.close();\n      } else this.open();\n    },\n    select: function select(num) {\n      this.index = num;\n      this.show = false;\n      this.$emit('selection', this.options[this.index].value);\n    }\n  },\n  computed: {\n    selected: function selected() {\n      if (this.show) {\n        return \"______\";\n      }\n\n      if (this.index == -1) {\n        return \"______\";\n      } else return this.options[this.index].text;\n    }\n  },\n  created: function created() {\n    if (!this.load) {\n      var title = this.setTitle;\n\n      for (var i = 0; i < this.options.length; i++) {\n        if (this.options[i].value == title) {\n          this.index = i;\n          break;\n        }\n      }\n    }\n\n    this.index = -1;\n  }\n};",{"version":3,"sources":["Select.vue"],"names":[],"mappings":";;;;;;;;;;AAUA;;;;;;AAMA,eAAA;AACA,EAAA,IAAA,EAAA,QADA;AAEA,EAAA,KAAA,EAAA;AACA,IAAA,OAAA,EAAA,KADA;AAEA,IAAA,IAAA,EAAA,OAFA;AAGA,IAAA,QAAA,EAAA,MAHA;AAIA,IAAA,QAAA,EAAA;AAJA,GAFA;AAQA,EAAA,IARA,kBAQA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,KADA;AAEA,MAAA,KAAA,EAAA,CAAA,CAFA;AAGA,MAAA,KAAA,EAAA;AAHA,KAAA;AAKA,GAdA;AAeA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,WAAA,KAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,KAJA,mBAIA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,KANA;AAOA,IAAA,IAPA,kBAOA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,SAAA;AACA,KAVA;AAWA,IAAA,KAXA,mBAWA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,KAbA;AAcA,IAAA,MAdA,oBAcA;AACA,UAAA,KAAA,IAAA,EACA;AACA,aAAA,KAAA,GAAA,CAAA,CAAA;AACA,aAAA,KAAA;AACA,OAJA,MAMA,KAAA,IAAA;AACA,KAtBA;AAuBA,IAAA,MAvBA,kBAuBA,GAvBA,EAuBA;AACA,WAAA,KAAA,GAAA,GAAA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,WAAA,EAAA,KAAA,OAAA,CAAA,KAAA,KAAA,EAAA,KAAA;AACA;AA3BA,GAfA;AA4CA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA,KAAA,IAAA,EACA;AACA,eAAA,QAAA;AACA;;AACA,UAAA,KAAA,KAAA,IAAA,CAAA,CAAA,EACA;AACA,eAAA,QAAA;AACA,OAHA,MAKA,OAAA,KAAA,OAAA,CAAA,KAAA,KAAA,EAAA,IAAA;AACA;AAZA,GA5CA;AA0DA,EAAA,OA1DA,qBA0DA;AACA,QAAA,CAAA,KAAA,IAAA,EAAA;AACA,UAAA,KAAA,GAAA,KAAA,QAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,OAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EACA;AACA,YAAA,KAAA,OAAA,CAAA,CAAA,EAAA,KAAA,IAAA,KAAA,EACA;AACA,eAAA,KAAA,GAAA,CAAA;AACA;AACA;AACA;AACA;;AACA,SAAA,KAAA,GAAA,CAAA,CAAA;AACA;AAvEA,CAAA","sourcesContent":["<template>\r\n    <div class=\"Select\">\r\n        <h1 :class=\"{throb: show || hover, load: load, throbempty: (load && index == -1)}\" @mouseover=\"hovering\" @mouseout=\"leave\" @click=\"toggle\">{{selected}}</h1>\r\n        <div v-if=\"show\" class=\"dropdown\">\r\n            <h1 @click=\"select(index)\" v-for=\"(option, index) in options\" :style=\"{'--delay': + index}\" :key=\"option.value\">{{option.text}}</h1>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n/*\r\ntypes: [\r\n        {value: \"\", text: \"Tracks\"},\r\n        {value: \"artists\", text: \"Artists\"},\r\n      ],\r\n*/\r\nexport default {\r\n    name: 'Select',\r\n    props: {\r\n        options: Array,\r\n        load: Boolean,\r\n        setTitle: String,\r\n        override: Boolean,\r\n    },\r\n    data() {\r\n        return {\r\n            show: false,\r\n            index: -1,\r\n            hover: false,\r\n        }\r\n    },\r\n    methods: {\r\n        hovering() {\r\n            this.hover = true;\r\n        },\r\n        leave() {\r\n            this.hover = false;\r\n        },\r\n        open() {\r\n            this.show = true;\r\n            this.$emit('pending');\r\n        },\r\n        close() {\r\n            this.show = false;\r\n        },\r\n        toggle() {\r\n            if (this.show)\r\n            {\r\n                this.index = -1;\r\n                this.close();\r\n            }\r\n            else\r\n                this.open();\r\n        },\r\n        select(num) {\r\n            this.index = num;\r\n            this.show = false;\r\n            this.$emit('selection', this.options[this.index].value);\r\n        }\r\n    },\r\n    computed: {\r\n        selected() {\r\n            if (this.show)\r\n            {\r\n                return \"______\";\r\n            }\r\n            if (this.index == -1)\r\n            {\r\n                return \"______\";\r\n            }\r\n            else\r\n                return this.options[this.index].text;\r\n        }\r\n    },\r\n    created() {\r\n        if (!this.load) {\r\n            let title = this.setTitle;\r\n            for (var i = 0; i < this.options.length; i++)\r\n            {\r\n                if (this.options[i].value == title)\r\n                {\r\n                    this.index = i;\r\n                    break;\r\n                }\r\n            }\r\n        }\r\n        this.index = -1;\r\n    }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.Select {\r\n    position: relative;\r\n    display: inline-block;\r\n}\r\n\r\n.Select h1 {\r\n    font-size: 40px;\r\n    font-weight: bolder;\r\n    color: rgb(255, 255, 255);\r\n    cursor: pointer;\r\n    margin: 0;\r\n    \r\n}\r\n\r\n\r\n.throb {\r\n    animation: throb 2s ease infinite;\r\n}\r\n\r\n.throbempty {\r\n    animation: throb-empty 2s ease infinite;\r\n}\r\n\r\n@keyframes throb {\r\n    50% {\r\n        opacity: .3;\r\n    }\r\n}\r\n\r\n@keyframes throb-empty {\r\n    0% {\r\n        opacity: 1;\r\n        text-shadow:01px 0px 10px white;\r\n    }\r\n    50% {\r\n        opacity: .1;\r\n        text-shadow: 0px 0px 0px white;\r\n    }\r\n    100% {\r\n        opacity: 1;\r\n        text-shadow: 0px 0px 10px white;\r\n    }\r\n}\r\n\r\n.dropdown {\r\n    position: absolute;\r\n    width: 300px;\r\n    top: 90%;\r\n    margin-top: 10px;\r\n    left: calc((100% - 300px) / 2);\r\n    z-index: 100;\r\n}\r\n\r\n.dropdown h1 {\r\n    --delay: 0;\r\n    margin: 5px 0px;\r\n    animation: slide-up .3s ease calc(var(--delay) * .1s), peekaboo calc(var(--delay) * .1s);\r\n}\r\n\r\n@keyframes slide-up {\r\n    from {\r\n        transform: translateY(50px);\r\n        opacity: 0;\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}
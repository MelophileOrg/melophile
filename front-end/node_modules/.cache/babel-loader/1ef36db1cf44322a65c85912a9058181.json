GAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA,CAAA,IAAA,CAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,QAAA,CAAA,CAAA,CAAA,GAAA,GAAA,IAAA,CAAA,IAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EACA,MAAA,IAAA,GAAA;AACA,YAAA,QAAA,CAAA,CAAA,CAAA,GAAA,EAAA,IAAA,CAAA,IAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EACA,MAAA,IAAA,GAAA;AACA,QAAA,MAAA,IAAA,QAAA,CAAA,CAAA,CAAA;AACA,YAAA,CAAA,IAAA,CAAA,EACA,MAAA,IAAA,GAAA;AACA;;AACA,aAAA,MAAA;AACA,KApBA;AAqBA,IAAA,kBArBA,8BAqBA,KArBA,EAqBA;AACA,UAAA,KAAA;;AACA,UAAA,MAAA,CAAA,UAAA,GAAA,GAAA,EAAA;AACA,QAAA,KAAA,GAAA,CAAA,MAAA,CAAA,UAAA,GAAA,GAAA,IAAA,CAAA,GAAA,EAAA;AACA,OAFA,MAGA;AACA,QAAA,KAAA,GAAA,MAAA,CAAA,UAAA,GAAA,EAAA;AACA;;AACA,aAAA;AACA,QAAA,KAAA,EAAA,KADA;AAEA,QAAA,KAAA,EAAA,KAFA;AAGA,QAAA,MAAA,EAAA;AAHA,OAAA;AAKA;AAlCA,GAVA;AA8CA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA;AACA,KAHA;AAIA,IAAA,MAJA,oBAIA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,MAAA;AACA,KANA;AAOA,IAAA,MAPA,oBAOA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,MAAA;AACA,KATA;AAUA,IAAA,OAVA,qBAUA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA;AACA,KAZA;AAaA,IAAA,SAbA,uBAaA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AACA,KAfA;AAgBA,IAAA,QAhBA,sBAgBA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA;AACA,KAlBA;AAmBA,IAAA,aAnBA,2BAmBA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,aAAA;AACA,KArBA;AAsBA,IAAA,IAtBA,kBAsBA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA;AACA,KAxBA;AAyBA,IAAA,SAzBA,uBAyBA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AACA;AA3BA,GA9CA;AA2EA,EAAA,OA3EA,qBA2EA,CACA;AA5EA,CAAA","sourcesContent":["<template>\n  <div class=\"bigpicture\">\n    <div id=\"page-title\">\n      <div id=\"big-picture\" class=\"page-title-icon\"></div>\n      <h1 id=\"page-title-text\">Your Big Picture</h1>\n    </div>\n\n    <div class=\"windows\" v-if=\"progress.total == progress.processed\">\n      <div class=\"window\">\n        <h1>Your Library:</h1>\n        <div class=\"flex flex-space-between\">\n          <h2>Tracks:</h2>\n          <h2 class=\"active\">{{formatNumber(Object.keys(tracks).length)}}</h2>\n        </div>\n        <div class=\"flex flex-space-between\">\n          <h2>Artists:</h2>\n          <h2 class=\"active\">{{formatNumber(Object.keys(artists).length)}}</h2>\n        </div>\n        <div class=\"flex flex-space-between\">\n          <h2>Genres:</h2>\n          <h2 class=\"active\">{{formatNumber(Object.keys(genres).length)}}</h2>\n        </div>\n      </div>\n\n      <div class=\"window\">\n        <h1>Characteristics:</h1>\n        <BarValue type=\"happiness\" :value=\"audioFeatures.valence.value\" maxLabel=\"Happy\" minLabel=\"Sad\" :color=\"audioFeatures.valence.color\"/>\n        <BarValue type=\"energy\" :value=\"audioFeatures.energy.value\" maxLabel=\"Hyper\" minLabel=\"Relaxed\" :color=\"audioFeatures.energy.color\"/>\n        <BarValue type=\"danceability\" :value=\"audioFeatures.danceability.value\" maxLabel=\"Danceable\" minLabel=\"Couch Potato\" :color=\"audioFeatures.danceability.color\"/>\n      </div>\n\n      <div class=\"window\">\n        <h1>Averages:</h1>\n        <BarValue type=\"tempo\" :value=\"audioFeatures.tempo.value\" maxLabel=\"Fast\" minLabel=\"Slow\" :color=\"audioFeatures.tempo.color\"/>\n        <BarValue type=\"major\" :value=\"mode.value\" maxLabel=\"Major\" minLabel=\"Minor\" :color=\"mode.major\"/>\n        <BarValue type=\"volume\" :value=\"audioFeatures.loudness.value\" maxLabel=\"Loud\" minLabel=\"Quiet\" :color=\"audioFeatures.loudness.color\"/>\n      </div>\n\n\n    </div>\n\n    \n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport BarValue from '@/components/Analysis/BarValue.vue'\n\n\nexport default {\n  name: 'bigpicture',\n  components: {\n    BarValue\n  },\n  data() {\n    return {\n\n    }\n  },\n  methods: {\n    formatNumber(num) {\n      let segments = [];\n      while (num >= 1000) {\n        let remainder = num % 1000;\n        segments.push(remainder);\n        num = Math.floor(num / 1000);\n      }\n      segments.push(num);\n      let string = \"\";\n      for (let i = segments.length - 1; i >= 0; i--) {\n        if (segments[i] < 100 && i != segments.length - 1) \n          string += \"0\";\n        if (segments[i] < 10 && i != segments.length - 1) \n          string += \"0\";\n        string += segments[i];\n        if (i != 0)\n          string += \",\";\n      }\n      return string;\n    },\n    featureDistibution(title) {\n      let width;\n      if (window.innerWidth > 720) {\n        width = (window.innerWidth - 260) / 2 - 60;\n      }\n      else {\n        width = window.innerWidth * .9;\n      }\n      return {\n        title: title,\n        width: width,\n        height: 225,\n      }\n    },\n  },\n  computed: {\n    progress() {\n      return this.$store.state.progress;\n    },\n    tracks() {\n      return this.$store.state.tracks;\n    },\n    genres() {\n      return this.$store.state.genres;\n    },\n    artists() {\n      return this.$store.state.artists;\n    },\n    topPlayed() {\n      return this.$store.state.topPlayed;\n    },\n    topSaved() {\n      return this.$store.state.topSaved;\n    },\n    audioFeatures() {\n      return this.$store.state.audioFeatures;\n    },\n    mode() {\n      return this.$store.state.mode;\n    },\n    dateAdded() {\n      return this.$store.state.dateAdded;\n    },\n  },\n  created() {\n  },\n}\n</script>\n\n<style scoped>\ndiv.v-tab {\n  justify-content: center;\n}\n\ndiv.v-card.theme--dark {\n  background: rgb(23, 37, 66) !important;\n}\n\n.theme--dark.v-tabs-items {\n  width: 100%;\n  background-color: rgb(23, 37, 66) !important;\n  background: rgb(23, 37, 66) !important;\n}\n\n.window div h2 {\n  color: rgba(161, 161, 161, 0.644);\n  font-size: 1em;\n  padding-bottom: 6px;\n}\n\n.window div h2.active {\n  color: rgba(255, 255, 255, 0.925);\n}\n\n</style>"],"sourceRoot":"src/views/Analysis"}]}
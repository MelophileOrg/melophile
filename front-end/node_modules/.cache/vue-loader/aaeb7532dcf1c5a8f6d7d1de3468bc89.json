rcent(num) {\n            return Math.round(num * 100) + \"%\";\n        },\n        valueCalc() {\n            if (this.type == \"tempo\")\n                return this.value / 210;\n            if (this.type == \"volume\")\n                \n            return Math.round(this.percent * 100) + \"%\"\n        },\n        valueText() {\n            if (this.type == \"tempo\")\n                return Math.round(this.value) + \"BPM\";\n            if (this.type == \"volume\")\n                return (Math.round(((this.value * 50) - 50) * 10) / 10) + \"dB\";\n            return Math.round(this.value * 100) + \"%\"\n        },\n    }\n};\n</script>\n\n<style scoped>\n.BarValue {\n    width: 100%;\n    margin: 12px 0px;\n}\n\n.bar {\n    position: relative;\n    width: 100%;\n    height: 12px;\n    border-radius: 6px;\n    background-color: rgba(255, 255, 255, 0.158);\n    overflow: hidden;\n}\n\n.bar p {\n    position: absolute;\n    left: 0;\n    top: 0;\n    width: 100%;\n    text-align: center;\n    margin: 0px;\n}\n\n.bar-fill {\n    --value: 1;\n    --red: 255;\n    --green: 255;\n    --blue: 255;\n    width: calc(var(--value) * 100%);\n    height: 100%;\n    background-color: rgb(var(--red), var(--green), var(--blue));\n}\n\n.labels {\n    display: flex;\n    justify-content: space-between;\n}\n\n.labels h3 {\n    display: inline-block;\n    color: rgba(255, 255, 255, 0.418);\n    font-size: .9em;\n    margin: 4px;\n    font-weight: lighter;\n}\n</style>"]}]}
unction toggle() {\n      if (this.show) {\n        this.index = -1;\n        this.close();\n      } else this.open();\n    },\n    select: function select(num) {\n      this.index = num;\n      this.show = false;\n      this.$emit('selection', this.options[this.index].value);\n    }\n  },\n  computed: {\n    selected: function selected() {\n      if (this.show) {\n        return \"______\";\n      }\n\n      if (this.index == -1) {\n        return \"______\";\n      } else return this.options[this.index].text;\n    }\n  },\n  created: function created() {\n    if (!this.load) {\n      var title = this.setTitle;\n\n      for (var i = 0; i < this.options.length; i++) {\n        if (this.options[i].value == title) {\n          this.index = i;\n          break;\n        }\n      }\n    }\n\n    this.index = -1;\n  }\n};",{"version":3,"sources":["Select.vue"],"names":[],"mappings":";;;;;;;;;;AAUA;;;;;;AAMA,eAAA;AACA,EAAA,IAAA,EAAA,QADA;AAEA,EAAA,KAAA,EAAA;AACA,IAAA,OAAA,EAAA,KADA;AAEA,IAAA,IAAA,EAAA,OAFA;AAGA,IAAA,QAAA,EAAA,MAHA;AAIA,IAAA,QAAA,EAAA;AAJA,GAFA;AAQA,EAAA,IARA,kBAQA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,KADA;AAEA,MAAA,KAAA,EAAA,CAAA,CAFA;AAGA,MAAA,KAAA,EAAA;AAHA,KAAA;AAKA,GAdA;AAeA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,WAAA,KAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,KAJA,mBAIA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,KANA;AAOA,IAAA,IAPA,kBAOA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,SAAA;AACA,KAVA;AAWA,IAAA,KAXA,mBAWA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,KAbA;AAcA,IAAA,MAdA,oBAcA;AACA,UAAA,KAAA,IAAA,EACA;AACA,aAAA,KAAA,GAAA,CAAA,CAAA;AACA,aAAA,KAAA;AACA,OAJA,MAMA,KAAA,IAAA;AACA,KAtBA;AAuBA,IAAA,MAvBA,kBAuBA,GAvBA,EAuBA;AACA,WAAA,KAAA,GAAA,GAAA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,WAAA,EAAA,KAAA,OAAA,CAAA,KAAA,KAAA,EAAA,KAAA;AACA;AA3BA,GAfA;AA4CA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA,KAAA,IAAA,EACA;AACA,eAAA,QAAA;AACA;;AACA,UAAA,KAAA,KAAA,IAAA,CAAA,CAAA,EACA;AACA,eAAA,QAAA;AACA,OAHA,MAKA,OAAA,KAAA,OAAA,CAAA,KAAA,KAAA,EAAA,IAAA;AACA;AAZA,GA5CA;AA0DA,EAAA,OA1DA,qBA0DA;AACA,QAAA,CAAA,KAAA,IAAA,EAAA;AACA,UAAA,KAAA,GAAA,KAAA,QAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,OAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EACA;AACA,YAAA,KAAA,OAAA,CAAA,CAAA,EAAA,KAAA,IAAA,KAAA,EACA;AACA,eAAA,KAAA,GAAA,CAAA;AACA;AACA;AACA;AACA;;AACA,SAAA,KAAA,GAAA,CAAA,CAAA;AACA;AAvEA,CAAA","sourcesContent":["<template>\n    <div class=\"Select\">\n        <h1 :class=\"{throb: show || hover, load: load, throbempty: (load && index == -1)}\" @mouseover=\"hovering\" @mouseout=\"leave\" @click=\"toggle\">{{selected}}</h1>\n        <div v-if=\"show\" class=\"dropdown\">\n            <h1 @click=\"select(index)\" v-for=\"(option, index) in options\" :style=\"{'--delay': + index}\" :key=\"option.value\">{{option.text}}</h1>\n        </div>\n    </div>\n</template>\n\n<script>\n/*\ntypes: [\n        {value: \"\", text: \"Tracks\"},\n        {value: \"artists\", text: \"Artists\"},\n      ],\n*/\nexport default {\n    name: 'Select',\n    props: {\n        options: Array,\n        load: Boolean,\n        setTitle: String,\n        override: Boolean,\n    },\n    data() {\n        return {\n            show: false,\n            index: -1,\n            hover: false,\n        }\n    },\n    methods: {\n        hovering() {\n            this.hover = true;\n        },\n        leave() {\n            this.hover = false;\n        },\n        open() {\n            this.show = true;\n            this.$emit('pending');\n        },\n        close() {\n            this.show = false;\n        },\n        toggle() {\n            if (this.show)\n            {\n                this.index = -1;\n                this.close();\n            }\n            else\n                this.open();\n        },\n        select(num) {\n            this.index = num;\n            this.show = false;\n            this.$emit('selection', this.options[this.index].value);\n        }\n    },\n    computed: {\n        selected() {\n            if (this.show)\n            {\n                return \"______\";\n            }\n            if (this.index == -1)\n            {\n                return \"______\";\n            }\n            else\n                return this.options[this.index].text;\n        }\n    },\n    created() {\n        if (!this.load) {\n            let title = this.setTitle;\n            for (var i = 0; i < this.options.length; i++)\n            {\n                if (this.options[i].value == title)\n                {\n                    this.index = i;\n                    break;\n                }\n            }\n        }\n        this.index = -1;\n    }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.Select {\n    position: relative;\n    display: inline-block;\n}\n\n.Select h1 {\n    font-size: 40px;\n    font-weight: bolder;\n    color: rgb(255, 255, 255);\n    cursor: pointer;\n    margin: 0;\n    \n}\n\n\n.throb {\n    animation: throb 2s ease infinite;\n}\n\n.throbempty {\n    animation: throb-empty 2s ease infinite;\n}\n\n@keyframes throb {\n    50% {\n        opacity: .3;\n    }\n}\n\n@keyframes throb-empty {\n    0% {\n        opacity: 1;\n        text-shadow:01px 0px 10px white;\n    }\n    50% {\n        opacity: .1;\n        text-shadow: 0px 0px 0px white;\n    }\n    100% {\n        opacity: 1;\n        text-shadow: 0px 0px 10px white;\n    }\n}\n\n.dropdown {\n    position: absolute;\n    width: 300px;\n    top: 90%;\n    margin-top: 10px;\n    left: calc((100% - 300px) / 2);\n    z-index: 100;\n}\n\n.dropdown h1 {\n    --delay: 0;\n    margin: 5px 0px;\n    animation: slide-up .3s ease calc(var(--delay) * .1s), peekaboo calc(var(--delay) * .1s);\n}\n\n@keyframes slide-up {\n    from {\n        transform: translateY(50px);\n        opacity: 0;\n    }\n}\n</style>\n"],"sourceRoot":"src/components"}]}